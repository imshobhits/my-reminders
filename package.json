{
  "name": "my-reminders",
  "version": "1.0.0",
  "description": "The package.json for the npm build dependencies.",
  "main": "build.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "requirejs": "^2.1.15",
    "bower": "^1.3.12",
    "less": "^2.5.3",
    "mustang": "^0.1.2",
    "ngrok": "^2.2.0",
    "npm": "^2.1.18",
    "mustache": "^1.0.0"
  },
  "devDependencies": {
    "@atlassian/micros-cli": "^5.16.5"
  },
  "scripts": {
    "build": "npm run build-css && npm run build-js",
    "build-css": "mkdir -p static-css && lessc --verbose static/less/app.less static-css/app.css",
    "build-js": "r.js -o build.js",
    "mustang": "mustang",
    "lessc": "lessc",
    "r.js": "r.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/atlassianlabs/my-reminders.git"
  },
  "keywords": [
    "my-reminders"
