    "@atlaskit/select": "^0.2.1",
    "@atlaskit/spinner": "^4.2.1",
    "@atlaskit/tooltip": "^8.4.1",
    "core-js": "^2.5.3",
    "moment": "^2.21.0",
    "moment-timezone-all": "^0.5.5",
    "portable-fetch": "^3.0.0",
    "rc-time-picker": "^3.3.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.3.0",
