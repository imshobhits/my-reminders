  "scripts": {
    "prestart": "npm run gen-fetch",
    "start": "react-scripts-ts start",
    "build": "npm run gen-fetch && npm run build-ts",
    "build-ts": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .storybook-static",
    "serve-storybook": "http-server -p 9002 .storybook-static",
    "get-swagger-codegen": "curl http://central.maven.org/maven2/io/swagger/swagger-codegen-cli/2.3.1/swagger-codegen-cli-2.3.1.jar -o swagger-codegen-cli.jar",
    "gen-fetch": "java -jar swagger-codegen-cli.jar generate -i ../swagger.yaml -l typescript-fetch -o src/reminders-client",
    "depcheck": "depcheck ."
  },
  "devDependencies": {
    "@atlaskit/css-reset": "^1.2.3",
    "@storybook/addon-actions": "^3.3.15",
